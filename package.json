{
    "name": "yelpcheetah",
    "displayName": "YelpCheetah Syntax",
    "description": "Syntax support for the Yelp Cheetah template language",
    "version": "1.0.1",
    "publisher": "joebateson",
    "repository": {
        "type": "git",
        "url": "https://github.com/jdb8/vscode-yelpcheetah.git"
    },
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cheetah",
                "aliases": [
                    "Yelp Cheetah",
                    "cheetah"
                ],
                "extensions": [
                    ".tmpl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cheetah",
                "scopeName": "text.html.yelp_cheetah",
                "path": "./syntaxes/YelpCheetah.tmLanguage"
            }
        ]
    }
}